<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/color_bg">
    <com.xuan.bigdog.lib.widgets.title.DGTitleLayout
        android:id="@+id/titleLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.xuan.bigappleui.lib.view.scrollview.BUVerticalScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dimen_60"
                android:layout_height="@dimen/dimen_138"
                android:background="@drawable/item_selector">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"/>
                <TextView
                    android:layout_marginLeft="@dimen/dimen_60"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    style="@style/FontBlack16"
                    android:text="@string/work_date"/>
                <TextView
                    android:id="@+id/dateTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    style="@style/FontGray16"
                    android:layout_marginRight="@dimen/dimen_48"
                    android:text="2016.06.14"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dimen_45"
                android:layout_height="@dimen/dimen_138"
                android:background="@drawable/item_selector">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"/>
                <TextView
                    android:layout_marginLeft="@dimen/dimen_60"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    style="@style/FontBlack16"
                    android:text="@string/work_up_time"/>
                <TextView
                    android:id="@+id/upTimeTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    style="@style/FontGray16"
                    android:layout_marginRight="@dimen/dimen_48"
                    android:text="08:55"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_138"
                android:background="@drawable/item_selector">
                <TextView
                    android:layout_marginLeft="@dimen/dimen_60"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    style="@style/FontBlack16"
                    android:text="@string/work_up_area"/>
                <TextView
                    android:id="@+id/upAreaTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    style="@style/FontGray16"
                    android:layout_marginRight="@dimen/dimen_48"
                    android:text="杭州市西湖区益乐路39号"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_selector"
                android:paddingBottom="@dimen/dimen_48"
                android:paddingRight="@dimen/dimen_60"
                android:paddingLeft="@dimen/dimen_60">
                <TextView
                    android:id="@+id/remarkView"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/FontBlack16"
                    android:layout_marginTop="@dimen/dimen_45"
                    android:text="@string/work_bz"/>
                <TextView
                    android:id="@+id/remarkTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/FontGray16"
                    android:layout_below="@id/remarkView"
                    android:layout_marginTop="@dimen/dimen_30"
                    android:text="无"
                    />
                <com.zjyeshi.dajiujiao.buyer.views.other.MyGridView
                    android:layout_below="@id/remarkTv"
                    android:layout_marginTop="@dimen/dimen_30"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/onGridView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:horizontalSpacing="10dp"
                    android:listSelector="@null"
                    android:numColumns="5"
                    android:verticalSpacing="10dp"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_divider"
                android:background="@color/color_divider"
                android:layout_alignParentBottom="true"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dimen_45"
                android:layout_height="@dimen/dimen_138"
                android:background="@drawable/item_selector">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"/>
                <TextView
                    android:layout_marginLeft="@dimen/dimen_60"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    style="@style/FontBlack16"
                    android:text="@string/work_down_time"/>
                <TextView
                    android:id="@+id/downTimeTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    style="@style/FontGray16"
                    android:layout_marginRight="@dimen/dimen_48"
                    android:text="18:55"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_138"
                android:background="@drawable/item_selector">
                <TextView
                    android:layout_marginLeft="@dimen/dimen_60"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    style="@style/FontBlack16"
                    android:text="@string/work_down_area"/>
                <TextView
                    android:id="@+id/downAreaTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    style="@style/FontGray16"
                    android:layout_marginRight="@dimen/dimen_48"
                    android:text="杭州市西湖区益乐路39号"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_divider"
                    android:background="@color/color_divider"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_selector"
                android:paddingBottom="@dimen/dimen_48"
                android:paddingRight="@dimen/dimen_60"
                android:paddingLeft="@dimen/dimen_60">
                <TextView
                    android:id="@+id/textView"
                    android:layout_toRightOf="@id/manageIv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/FontBlack16"
                    android:layout_marginTop="@dimen/dimen_45"
                    android:text="@string/work_bz"/>
                <TextView
                    android:id="@+id/bzTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/FontGray16"
                    android:layout_below="@id/textView"
                    android:layout_marginTop="@dimen/dimen_30"
                    android:text="无"
                    />
                <com.zjyeshi.dajiujiao.buyer.views.other.MyGridView
                    android:layout_below="@id/bzTv"
                    android:layout_marginTop="@dimen/dimen_30"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/offGridView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:horizontalSpacing="10dp"
                    android:listSelector="@null"
                    android:numColumns="5"
                    android:verticalSpacing="10dp"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_divider"
                android:background="@color/color_divider"
                android:layout_alignParentBottom="true"/>
        </LinearLayout>
    </com.xuan.bigappleui.lib.view.scrollview.BUVerticalScrollView>
</LinearLayout>